const myAge = 33; //I am soooo old...
let earlyYears = 2; //some random variable
earlyYears*=10.5;
let laterYears = myAge - 2; // deductions?
laterYears*=4; //multiply by 4
console.log(`earlyYears = ${earlyYears} \nlaterYears = ${laterYears}`);
let myAgeInDogYears = earlyYears + laterYears; //my age in dog years I guess, preposterous!
const myName = `Paul`.toLowerCase();
console.log(`My name is ${myName}. I am ${myAgeInDogYears} years old in dog years.`); //just a console printout of earlier defined variables
